{
  "description": "Fumble and mess the Http Response",
  "manifest_version": 2,
  "name": "Http Response Tweaker",
  "version": "0.2.4",
  "homepage_url": "https://github.com/naokiri/HttpResponseTweaker",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "background.js"
    ]
  },
  "devtools_page": "devtools/devtools.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }
}
